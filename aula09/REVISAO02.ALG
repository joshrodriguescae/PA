Algoritmo "revisao02"


Var
num:inteiro



Inicio
 escreval("Digite um número inteiro")
leia(num)

se (num > 0) entao
   escreval("o número",num," é positivo")
senao
 se (num < 0) entao
   escreval("o número",num," é negativo")
senao
  escreval("o número",num," é neutro")
fimse
 fimse

Fimalgoritmo